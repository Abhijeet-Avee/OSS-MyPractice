package multithreading;

class Q {
	int n;

	int get() {
		System.out.println("get: "+n);
		return n;
	}
	void put(int n) {
		this.n=n;
		System.out.println("put: "+n);
	}
}

class Producer implements Runnable{
	Q q;
	
	public Producer(Q q) {
		this.q = q;
		new Thread(this).start();
	}
	
	@Override
	public void run() {
		
	}
	
}

public class ProducerAndConsumer {
	public static void main(String[] args) {

	}
}
