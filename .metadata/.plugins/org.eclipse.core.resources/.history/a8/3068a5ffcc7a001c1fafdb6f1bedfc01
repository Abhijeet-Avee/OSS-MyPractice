package mypractice.dp;


//Approach: LAZY CREATION OF OBJECT
//Problem: 
class Singleton2{
	private static Singleton2 singleton1 ;
	private Singleton2() {
		singleton1 = new Singleton2();
	}
	public static Singleton2 getSingleton2() {
		return singleton1;
	}
}

public class SingletonDP2 {
	public static void main(String[] args) {
		Singleton2 singleton1 = Singleton2.getSingleton2(); 
		Singleton2 singleton2 = Singleton2.getSingleton2();
		
		System.out.println(singleton1.hashCode());
		System.out.println(singleton2.hashCode());
	}
}
