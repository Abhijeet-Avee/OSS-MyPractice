package mypractice.dp;

class Singleton7 {
	private static volatile Singleton6 singleton1 = null;

	private Singleton7() {
	}

	public static Singleton7 getSingleton7() {
		if (singleton1 == null) {
			if (singleton1 == null) {
				synchronized (Singleton7.class) {
					singleton1 = new Singleton7();
				}
			}
		}
		return singleton1;
	}
}

public class SingletonDP7 {

}
